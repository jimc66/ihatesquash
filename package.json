{
  "name": "ihatesquash",
  "version": "1.0.0",
  "description": "A passionate defense against the tyranny of squash",
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently \"npm run build:css\" \"npm run serve\"",
    "build": "npm run build:css && npx @11ty/eleventy",
    "serve": "npx @11ty/eleventy --serve",
    "build:css": "npx tailwindcss -i ./src/styles/input.css -o ./_site/styles/output.css --watch",
    "build:css:prod": "NODE_ENV=production npx tailwindcss -i ./src/styles/input.css -o ./_site/styles/output.css --minify"
  },
  "keywords": ["squash", "vegetables", "humor"],
  "author": "Jim Cuff",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "concurrently": "^8.2.2",
    "daisyui": "^5.5.5",
    "tailwindcss": "^3.3.5"
  }
}
